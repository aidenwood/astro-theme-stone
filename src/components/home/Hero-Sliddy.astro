<style>

  .carousel-cell {
    margin-right: 32px;
    width: calc(33% - 16px);
    max-height: 70vh;
    filter: drop-shadow(0 1px 6px #00000015);
  }
  @media only screen and (max-width: 1980px) {
    .carousel-cell {
      width: calc(45% - 16px);
    }
  }
  @media only screen and (max-width: 1440px) {
    .carousel-cell {
      width: calc(65% - 16px);
    }
  }
  @media only screen and (max-width: 1199px) {
    .carousel-cell {
      width: 75%;
